    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.19.1",
    "@types/express": "^4.17.15",
    "@types/node": "^18.11.18",
    "@types/node-cron": "^3.0.7",
    "@types/parse": "^3.0.1",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "eslint": "^8.30.0",
    "jasmine": "4.5.0",
    "mongodb-runner": "4.9.0",
