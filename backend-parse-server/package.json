{
  "name": "shift",
  "version": "1.0.0",
  "description": "A parse-server application for creating shift plans.",
  "repository": {
    "type": "git",
    "url": "https://github.com/biersoeckli/shift"
  },
  "license": "MIT",
  "main": "index.js",
  "scripts": {
    "coverage": "nyc jasmine",
